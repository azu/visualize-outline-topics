{
  "name": "visualize-outline-topics",
  "version": "1.0.0",
  "description": "Visualize outline's topics.",
  "homepage": "https://github.com/azu/visualize-outline-topics",
  "bugs": {
    "url": "https://github.com/azu/visualize-outline-topics/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/visualize-outline-topics.git"
  },
  "license": "MIT",
  "author": "azu",
  "sideEffects": false,
  "files": [
    "bin/",
    "lib/",
    "module/",
    "src/"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "tsc -p . && tsc -p ./tsconfig.module.json",
    "start": "next start",
    "lint": "next lint",
    "test": "mocha \"test/**/*.ts\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks",
    "watch": "tsc -p . --watch",
    "clean": "rimraf lib/ module/",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "devDependencies": {
    "@types/base64-url": "^2.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "eslint": "<8.0.0",
    "eslint-config-next": "12.0.2",
    "lint-staged": "^11.2.6",
    "mocha": "^9.1.3",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "ts-node-test-register": "^10.0.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.0.6",
    "base64-url": "^2.3.3",
    "easymde": "^2.15.0",
    "fastest-levenshtein": "^1.0.12",
    "immer": "^9.0.6",
    "mdast-util-to-string": "^3.1.0",
    "next": "^12.0.2",
    "next-remove-imports": "^1.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^7.1.0",
    "react-markdown-editor-lite": "^1.3.1",
    "react-simplemde-editor": "^5.0.2",
    "remark-gfm": "^3.0.0",
    "remark-parse": "^10.0.0",
    "remark-rehype": "^10.0.0",
    "unified": "^10.1.0",
    "unist-util-find-after": "^4.0.0",
    "unist-util-select": "^4.0.1",
    "use-local-storage-state": "^11.0.0"
  }
}
